{
  "name": "@maherunlocker/custom-react-table-responsive",
  "version": "2.0.0",
  "description": "dynamic table based on react table v7",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "lint": "npm lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaherUnlocker/custum-react-table.git"
  },
  "dependencies": {
    "@aureskonnect/react-ui": "latest",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@ggascoigne/namor": "^2.0.2",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@mui/styles": "^5.4.2",
    "axios": "0.26.1",
    "classnames": "^2.3.1",
    "framer-motion": "4.1.17",
    "lodash.concat": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.without": "^4.4.0",
    "match-sorter": "^6.3.0",
    "react-json-view": "^1.21.3",
    "react-table": "^7.7.0",
    "tss-react": "^3.3.1"
  },
  "devDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/classnames": "^2.3.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.23",
    "@types/lodash.concat": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/lodash.without": "^4.4.6",
    "@types/node": "^16.0.0",
    "@types/prettier": "^2.3.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-table": "^7.7.1",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@types/testing-library__react": "^10.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "import-sort-cli": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-error-overlay": "6.0.9",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.3.5",
    "typesync": "^0.8.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "prettier --no-color --write",
      "yarn lint"
    ],
    "*.{css,scss,graphql}": [
      "prettier --no-color --write"
    ]
  }
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "keywords": [
    "react",
    "table",
    "mui",
    "5",
    "reactsrap",
    "typescript"
  ],
  "author": "Maher BEN BACCAR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaherUnlocker/custum-react-table/issues"
  },
  "homepage": "https://github.com/MaherUnlocker/custum-react-table#readme"
}